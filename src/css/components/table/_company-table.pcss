.company-table {
  th {
    line-height: 1.2em;
  }
  .col-position,
  .col-company {
    width: 17%;
  }
  .col-type {
    width: 9%;
  }
  .col-day-time {
    width: 8%;
  }
  .col-week-time {
    width: 13%;
  }
  .col-frequency {
    width: 9%;
  }
  .col-experience {
    width: 7%;
  }
  .col-salary {
    width: 10%;
  }
  .col-time {
    width: 8%;
  }
  &.rwd-table th {
    padding-top: 15px;
  }
  .data-time {
    font-size: 0.95em;
    color: $gray;
  }
}
